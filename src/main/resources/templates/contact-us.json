"Template": {
    "HtmlPart":"<!DOCTYPE html>\r\n<html>\r\n<head>\r\n  <title>New Cleaning Service Appointment</title>\r\n  <style>\r\n    body {\r\n      font-family: Arial, sans-serif;\r\n      margin: 0;\r\n      padding: 0;\r\n    }\r\n    body img {\r\n        width: 30%;\r\n    }\r\n    @media screen and (max-width: 700px) {\r\n        body img {\r\n            width: 70%;\r\n        }\r\n    }\r\n    @media screen and (max-width: 980px) {\r\n        body img {\r\n            width: 40%;\r\n        }\r\n    }\r\n    .container {\r\n      margin: 20px auto;\r\n      padding: 20px;\r\n      width: 80%;\r\n      border: 1px solid #ccc;\r\n      border-radius: 5px;\r\n      background-color: #f9f9f9;\r\n    }\r\n    .header {\r\n      text-align: center;\r\n      background-color: #f9f9f9;\r\n      color: white;\r\n      padding: 10px;\r\n      border-radius: 5px 5px 0 0;\r\n    }\r\n    .content {\r\n      margin: 20px;\r\n    }\r\n    .footer {\r\n      text-align: center;\r\n      background-color: #333;\r\n      color: white;\r\n      padding: 10px;\r\n      border-radius: 0 0 5px 5px;\r\n    }\r\n  </style>\r\n</head>\r\n<body>\r\n\r\n<div class=\"container\">\r\n  <div class=\"content\">\r\n    <p>Hello,</p>\r\n    <p>A potential customer wants to contact you! </p>\r\n    <p><strong>First Name:</strong> {{firstName}}</p>\r\n    <p><strong>Last Name:</strong> {{lastName}}</p>\r\n    <p><strong>Reason for Contact:</strong> {{reasonForContact}}</p>\r\n    <p><strong>Email:</strong> {{email}}</p>\r\n    <p><strong>Message:</strong> {{message}}</p>\r\n  </div>\r\n  <div class=\"footer\">\r\n    <h2>This message is automated</h2>\r\n  </div>\r\n</div>\r\n</body>\r\n</html>",
